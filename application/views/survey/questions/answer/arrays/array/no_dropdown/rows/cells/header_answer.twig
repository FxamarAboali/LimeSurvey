{#
/**
 * Array header : answer or sub question
 * @var $class
 * @var $content
 * @var $basename
 * @var $code
 * @var $isrepeat
 * @var $oAnswer
 */
#}
<th class="{{ class }} {% if even %} ls-even{% else %} ls-odd{% endif %}" {% if not isrepeat %} id="answer{{ basename }}-{{ code }}" role="columnheader" {% endif %}>
	{{ processString(content) }}
</th>
